{
  "name": "bedloop",
  "version": "0.1.0-alpha.6",
  "type": "module",
  "description": "TypeScript client for Bedloop API with real-time messaging and booking management",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "playground": "tsx playground/playground.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.7.3",
    "tsup": "8.5.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  },
  "keywords": [
    "bedloop",
    "api",
    "client",
    "typescript",
    "messaging",
    "booking",
    "real-time",
    "polling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johan12361/bedloop.git"
  },
  "bugs": {
    "url": "https://github.com/johan12361/bedloop/issues"
  },
  "homepage": "https://github.com/johan12361/bedloop#readme",
  "author": "johan12361",
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "axios": "1.13.2"
  }
}
